{
    "parser": "babel-eslint",
    "env": {
        "mocha": true,
        "node": true,
    },
    "extends": "airbnb-base",
    "rules": {
        "func-names": "off",
        "import/no-extraneous-dependencies": "off",
        "indent": ["error", 4],
        "max-len": ["error", {"code": 120, "tabWidth": 4, "ignoreUrls": true}],
        "no-console": "off",
    },
}
